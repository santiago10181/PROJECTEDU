{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "aulaVirtual/dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.js" },
      { "src": "/aulaVirtual/dist/(.*)", "dest": "/aulaVirtual/dist/$1" }
    ]
  }